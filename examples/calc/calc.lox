@token 
  NUM

  PLUS     '+'
  MINUS    '-'
  MUL      '*'
  DIV      '/'
  REM      '%'
  POW      '^'
  O_PAREN  '('
  C_PAREN  ')' ;

S = expr ;

expr = expr PLUS expr   @left(1)
     | expr MINUS expr  @left(1)
     | expr MUL expr    @left(2)
     | expr DIV expr    @left(2)
     | expr REM expr    @left(2)
     | expr POW expr    @right(3)
     | O_PAREN expr C_PAREN
     | num ;

num = NUM
    | MINUS NUM ;
