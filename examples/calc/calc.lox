@token
  NUM      'number'

  PLUS     '+'
  MINUS    '-'
  MUL      '*'
  DIV      '/'
  REM      '%'
  POW      '^'
  O_PAREN  '('
  C_PAREN  ')' ;

S = expr ;

expr = expr '+' expr    @left(1)
     | expr '-' expr    @left(1)
     | expr '*' expr    @left(2)
     | expr '/' expr    @left(2)
     | expr '%' expr    @left(2)
     | expr '^' expr    @right(3)
     | '(' expr ')'
     | num ;

num = NUM
    | MINUS NUM ;
