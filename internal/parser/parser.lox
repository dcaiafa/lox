@token
  ID
  LITERAL
  NUM

  ZERO_OR_MANY
  ONE_OR_MANY
  ZERO_OR_ONE

  DEFINE
  OR
  SEMICOLON
  OPAREN
  CPAREN

  TOKEN
  LEFT
  RIGHT ;

Parser   = Pdecl* ;
Pdecl    = Prule | Ltoken;
Prule    = ID DEFINE Pprods SEMICOLON ;
Pprods   = Pprods OR Pprod
         | Pprod ;
Pprod    = Pterm+ Pqualif? ;
Pterm    = ID Pcard? ;
Pcard    = ZERO_OR_MANY | ONE_OR_MANY | ZERO_OR_ONE ; 
Pqualif  = LEFT OPAREN NUM CPAREN
         | RIGHT OPAREN NUM CPAREN ;
Ltoken   = TOKEN ID+ SEMICOLON ;
